<div class="row">
  <div class="col-md-12">
    <div class="col-md-6">
      <h2>Images</h2>
    </div>
    <div class="col-md-6">
      <div class="model_buttons">
        <%= link_to "Add New", new_admin_gallery_path, class: 'btn btn-primary' %>
      </div>
    </div>
  </div>
</div>
</br>
<div class="row">

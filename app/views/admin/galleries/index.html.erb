    <li class="active">
      <a href="#slides" data-toggle="tab">Slides</a>
    </li>
    <li><a href="#gallery" data-toggle="tab">Gallery</a></li>
  </ul>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="tab-content">
      <div id="slides" class="tab-pane fade in active">
        <%= render "slider",locals: {galleries: @galleries} %>
      </div>
      <div id="gallery" class="tab-pane fade">
        <%= render "gallery",locals: {galleries: @galleries} %>
      </div>
    </div>
  </div>
</div>
